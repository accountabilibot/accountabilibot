{
    "name": "accountabilibot-discord-e2e",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "implicitDependencies": ["accountabilibot-discord"],
    "targets": {
        "e2e": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
            "options": {
                "jestConfig": "packages/accountabilibot-discord-e2e/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "lint": {
            "executor": "@nx/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": [
                    "packages/accountabilibot-discord-e2e/**/*.{js,ts}"
                ]
            }
        }
    }
}
